<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="pegasusos"
           fetch="https://github.com/PegasusOS/"
           revision="refs/heads/11" />

  <!-- Art -->
  <project path="art" name="android_art" groups="pdk" remote="pegasusos" />

  <!-- Build -->
  <project path="build/make" name="android_build" groups="pdk" remote="pegasusos" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/blueprint" name="android_build_blueprint" groups="pdk,tradefed" remote="pegasusos" />
  <project path="build/soong" name="android_build_soong" groups="pdk,tradefed" remote="pegasusos" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

